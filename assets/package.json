{
  "name": "react-webpack-boilerplate",
  "version": "2.0.0",
  "description": "Minimalistic ES6 React boilerplate",
  "main": "index.js",
  "scripts": {
    "prestart": "npm run production",
    "start": "webpack-dev-server",
    "production": "webpack --config webpack.production.config.js --progress --profile --colors",
    "lint": "eslint app",
    "test": "echo \"Not implemented yet\nmocha --compilers js:babel-core/register --recursive ./tests/**/*.js\"",
    "precommit": "cross-env npm run lint && npm test",
    "prepush": "cross-env npm run lint && npm test"
  },
  "author": "Dmitriy Chernyshov <dmitriy.chernyshof@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "cross-env": "5.0.0",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "react-hot-loader": "3.0.0-beta.6",
    "react-router-dom": "4.1.1"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.0.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-boilerplate": "1.1.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-2": "6.24.1",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "0.28.1",
    "enzyme": "2.8.2",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "15.0.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "7.0.1",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.11.1",
    "html-webpack-plugin": "2.28.0",
    "husky": "0.13.3",
    "node-sass": "4.5.3",
    "open-browser-webpack-plugin": "0.0.5",
    "postcss-loader": "2.0.5",
    "react-addons-test-utils": "15.5.1",
    "sass-loader": "6.0.5",
    "style-loader": "0.17.0",
    "url-loader": "0.5.8",
    "webpack": "2.5.1",
    "webpack-dev-server": "2.4.5"
  }
